# Prometheus / Mimir connection
PROMETHEUS_URL=http://prometheus:9090
# Optional: basic auth user and password (both must be set for auth to work)
PROMETHEUS_USER=
PROMETHEUS_PASSWORD=
PROMETHEUS_INSECURE=
PROMETHEUS_TIMEOUT=10
# Step resolution for query_range (should match scrape_interval, default: 15s)
PROMETHEUS_QUERY_STEP_SECONDS=15

# ClickHouse connection
CLICKHOUSE_URL=http://clickhouse:8123
CLICKHOUSE_USER=
CLICKHOUSE_PASSWORD=
CLICKHOUSE_CONNECT_TIMEOUT=10
CLICKHOUSE_SEND_RECEIVE_TIMEOUT=300
CLICKHOUSE_INSECURE=
CLICKHOUSE_TABLE=metrics.prometheus_raw

# PushGateway connection
PUSHGATEWAY_URL=http://pushgateway:9091
PUSHGATEWAY_TOKEN=
PUSHGATEWAY_USER=
PUSHGATEWAY_PASS=
PUSHGATEWAY_TIMEOUT=10
PUSHGATEWAY_INSECURE=
PUSHGATEWAY_JOB=etl_prometheus2clickhouse
PUSHGATEWAY_INSTANCE=etl_prometheus2clickhouse

# ETL settings
BATCH_WINDOW_SECONDS=300

# Logging
LOG_LEVEL=INFO
